    "types": "dist/sudokuSolverAsync.d.ts",
    "author": "christopher.pkm@gmail.com",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "typedoc": "^0.27.6",
        "typedoc-plugin-markdown": "^4.4.1",
        "typescript": "^5.7.3",
        "prettier": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/zoolu-got-rhythm/sudoku-solver-async.git"
    },
    "bugs": {
        "url": "https://github.com/zoolu-got-rhythm/sudoku-solver-async/issues"
    },
    "homepage": "https://github.com/zoolu-got-rhythm/sudoku-solver-async/blob/main/README.md",
    "scripts": {
        "build": "tsc",
        "start": "npm run build && node dist/index.js",
        "test": "jest",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint . --ext .ts",
        "prepare": "npm run build",
        "postversion": "git push && git push --tags"
    },
    "files": [
        "dist/sudokuSolverAsync.js",
        "dist/sudokuSolverAsync.js.map",
